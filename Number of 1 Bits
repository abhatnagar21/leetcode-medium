Number of 1 Bits
class Solution {
public:
    int hammingWeight(uint32_t n) {
        /*int cnt=0;
        for(int i=0;i<=31;i++){
        if((n & (1 << i)) !=0)
        cnt++;}
        return cnt;*/
        int bit=0;
        while(n!=0)
        {
            bit++;
            n=n&(n-1);
        }
        return bit;
    }
};
